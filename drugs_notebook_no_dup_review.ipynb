    "def filter_review(x):\n",
    "    text = word_tokenize(x)\n",
    "    # Apply an steemer\n",
    "    tags = nltk.pos_tag(text)\n",
    "    words = []\n",
    "    for tag in tags:\n",
    "        # Include the type of words that you want to consider.\n",
    "        # It will be also better \n",
    "        if ( tag[1] in ['JJ','NN'] ):\n",
    "            words.append(tag[0])\n",
    "    return words[1:-1]"
   ]
